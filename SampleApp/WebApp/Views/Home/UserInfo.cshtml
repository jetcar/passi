@{
    ViewData["Title"] = "User Info";
}

<div class="text-center">
    <h1 class="display-4">User Info</h1>

    <div class="form-group row">
        <label for="userId" class="col-sm-2 col-form-label">UserID</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="userId" value="@Model.UserId" disabled=""></input>
        </div>
    </div>

    <div class="form-group row">
        <label for="username" class="col-sm-2 col-form-label">Thumbprint</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="username" value="@Model.Thumbprint" disabled=""></input>
        </div>
    </div>

    <div class="form-group row">
        <label for="email" class="col-sm-2 col-form-label">Validfrom</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="email" value="@Model.ValidFrom" disabled=""></input>
        </div>
    </div>
    <div class="form-group row">
        <label for="email" class="col-sm-2 col-form-label">Validto</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="email" value="@Model.ValidTo" disabled=""></input>
        </div>
    </div>
    <div class="form-group row">
        <label for="email" class="col-sm-2 col-form-label">Public Cert</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="email" value="@Model.PublicCert" disabled=""></input>
        </div>
    </div><div class="form-group row">
        <label for="email" class="col-sm-2 col-form-label">SignedHash</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="email" value="@Model.SignedHash" disabled=""></input>
        </div>
    </div>
    
</div>