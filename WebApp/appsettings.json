{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Elastic.Apm": "Debug"
    }
  },

  "AppSetting": {
    "IdentityUrl": "https://192.168.0.208/identity",
    "returnUrl": "/oauth/callback",
    "WebAppDbName": "WebApp",
    "DbHost": "localhost",
    "DbUser": "postgres",
    "DbPassword": "test1",
    "ClientId": "SampleApp",
    "ClientSecret": "secret",
    "ClientsPage": "/client",
    "PassiUrl": "http://192.168.0.208:5004/passiapi",
    "DbSslMode": "Allow",
    "SendgridApiKey": "",
    "projectId": "projectId"
  },
  "Urls": "http://0.0.0.0:5002",

  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.GoogleCloudLogging" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "GoogleCloudLogging",
        "Args": {
          "projectID": "passi-165ca",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],

    "Properties": {
      "Application": "Passi webapi"
    }
  }
}