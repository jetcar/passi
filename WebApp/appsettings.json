{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Elastic.Apm": "Debug"
    }
  },
  "AppSetting": {
    "openIdcUrl": "https://localhost:44310/",
    "returnUrl": "/oauth/callback",
    "WebAppDbName": "WebApp",
    "DbHost": "localhost",
    "DbUser": "postgres",
    "DbPassword": "test1",
    "DbPort": 5432,
    "RedisHost": "localhost",
    "RedisPort": "6379",
    "RedisPassword": "",
    "ClientId": "SampleApp",
    "ClientSecret": "secret",
    "ClientsPage": "/client",
    "PassiUrl": "http://192.168.0.208:5004/passiapi",
    "DbSslMode": "Allow",
    "SendgridApiKey": "",
    "projectId": "projectId"
  },
  "Urls": "https://0.0.0.0:5002",
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.GoogleCloudLogging"
    ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "GoogleCloudLogging",
        "Args": {
          "projectID": "passi-165ca",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "Passi webapi"
    }
  }
}