<?xml version="1.0" encoding="utf-8" ?>
<ContentPage  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
              xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
              xmlns:fingerPrint="clr-namespace:passi_maui.FingerPrint"
              x:DataType="fingerPrint:FingerPrintView"
              x:Class="passi_maui.FingerPrint.FingerPrintView">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" VerticalOptions="Center" Orientation="Vertical">
            <Label HorizontalTextAlignment="Center" TextColor="Black" Margin="10" Text="{Binding Path=Message}"></Label>
            <Image Source="baseline_fingerprint_black_24.png"></Image>
            <Button Background="White" CornerRadius="100"  Text="Cancel" Clicked="CancelButton_OnClicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>